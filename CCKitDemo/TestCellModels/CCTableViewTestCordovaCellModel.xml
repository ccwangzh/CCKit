<?xml version='1.0' encoding='utf-8'?>
<widget id="com.womob.CCKitDemo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CCKitDemo</name>
    <description>A sample CCKit application</description>
    <author email="ccwangzh@sina.com" href="http://cordova.io">ccwangzh</author>

    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    
    <feature name="cordovaLoginHandler">
        <param name="ios-package" value="CCCordovaLoginHandler" />
        <param name="onload" value="true" />
    </feature>

    <feature name="cordovaShareHandler">
        <param name="ios-package" value="CCCordovaShareHandler" />
        <param name="onload" value="true" />
    </feature>

    <allow-navigation href="*" />
</widget>
